@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<!-- Start main Section -->
<section class="ec-404-error-page-02">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 text-center">
                <img class="img-404" src="~/Theme/assets/images/common/404-error-page.gif" alt="" />

                <h1 class="main-title text-center">404</h1>
                <h3 class="sub-title text-center">Something Went Wrong</h3>

                <p class="desc-content text-center">the page you are looking for not avaible!</p>

                <a class="link-404" href="@Url.Action("Index", "Home")">Go to Back</a>
            </div>
        </div>
    </div>
</section>
<!-- End main Section -->
